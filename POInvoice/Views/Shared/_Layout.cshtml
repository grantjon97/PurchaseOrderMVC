<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>PO Form Manager</title>
    <link href="http://cdn.wbp.org/CPS-Images/lcms_cross.gif" rel="icon">

    <!-- Stylesheets -->
    <link href="http://cdn.wbp.org/1.0.0/styles/cps.all.min.css" rel="stylesheet">
    <link href="http://cdn.wbp.org/1.0.0/styles/cps-lite.min.css" rel="stylesheet">
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/typeahead.css" rel="stylesheet" />

    @Html.Partial("~/Views/Shared/Partials/_UploadTemplate.cshtml")

    <!-- Scripts -->
    <script src="http://cdn.wbp.org/1.0.0/scripts/cps.scripts.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/CreateCalendars.js"></script>
    <script src="~/Scripts/UploadOldPo.js"></script>
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>
    <input hidden id="userName" value="@(POInvoice.Services.PoFormConversions.FormatUserName(System.Web.HttpContext.Current.User.Identity.Name))" />
    @Html.Partial("~/Views/Shared/Partials/_NavBar.cshtml")
    <div class="container body-content">
        <br />
        @RenderBody()

        <div id="bottomPageStuff">
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - All rights reserved.</p>
            </footer>
        </div>
    </div>
</body>

</html>
